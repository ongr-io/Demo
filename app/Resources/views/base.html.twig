<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{{ "ONGR Demo" }}{% endblock %}</title>
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/ongr.css') }}"/>
        {% endblock %}
        {% block canonical_meta %}{% endblock %}
    </head>
    <body>
    {% block init %}{% endblock %}
    {% include "::inc/topbar.html.twig" %}
    <div class="container">
        {% block container %}
            {% block page_head %}
                {% include "::inc/header.html.twig" %}
            {% endblock %}

            {% block top_navigation %}
                {% include "::inc/navbar.html.twig" %}
            {% endblock %}
            {% block content_wrapper %}
                <div class="row">
                    <div class="col-sm-12">
                        {% block breadcrumbs %}
                            <ol class="breadcrumb">
                                {#<li><a href="{{ path('app_homepage') }}">{{ 'Home'|trans }}</a></li>#}
                                {% block breadcrumbs_inner %}{% endblock %}
                            </ol>
                        {% endblock %}
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        {% block sidebar %}
                        {% endblock %}
                    </div>
                    <div class="col-sm-9">
                        {% block content %}{% endblock %}
                    </div>
                </div>
            {% endblock %}
        {% endblock %}
    </div>
    <div class="scroll-top">
        <a href="#" class="scroll-top-link">
            <svg viewBox="0 0 20 20">
                <path d="M13.418 7.859c0.271-0.268 0.709-0.268 0.978 0s0.272 0.701 0 0.969l-3.908 3.83c-0.27 0.268-0.707 0.268-0.979 0l-3.908-3.83c-0.27-0.267-0.27-0.701 0-0.969s0.709-0.268 0.978 0l3.421 3.141 3.418-3.141z"></path>
            </svg>
        </a>
    </div>
    {% include "::inc/footer.html.twig" %}
    {% block javascripts %}
        <script>
            var appLocale = "{{ app.request.locale }}";
        </script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
        <script src="{{ asset('js/ongr.js') }}"></script>
    {% endblock %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-74352978-1', 'auto');
        ga('send', 'pageview');
    </script>
    </body>
</html>
